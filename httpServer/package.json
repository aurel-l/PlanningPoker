{
  "name": "planning-poker-http-server",
  "version": "0.0.5",
  "scripts": {
    "test": "npm-run-all test:*",
    "test:lint": "eslint --ignore-path ../.gitignore '**/*.js'",
    "test:flow": "flow check",
    "test:unit": "echo \"Warning, no unit test specified yet, passing\" && exit 0",
    "test:functional": "echo \"Warning, no functional test specified yet, passing\" && exit 0",
    "bundle": "webpack --progress",
    "cleanup": "rimraf dist && mkdirp dist",
    "predeploy": "npm-run-all lint test cleanup",
    "deploy": "NODE_ENV=production npm-run-all -p deploy:*",
    "deploy:static": "cpx \"src/**/*.html\" dist",
    "deploy:webpack": "npm run bundle -- -p",
    "postdeploy": "npm run test",
    "prestart": "npm run cleanup",
    "start": "npm-run-all -p start:*",
    "start:static": "npm run deploy:static -- --watch",
    "start:webpack": "npm run bundle -- --watch -d"
  },
  "author": "Aur√©lien Luciani",
  "dependencies": {
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.0",
    "redux": "^3.3.1",
    "socket.io-client": "^1.4.5"
  },
  "devDependencies": {
    "babel-core": "^6.6.0",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.6.3",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "cpx": "^1.2.1",
    "css-loader": "^0.23.1",
    "empty": "^0.10.1",
    "eslint": "^2.2.0",
    "eslint-plugin-import": "^1.0.3",
    "eslint-plugin-react": "^4.1.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "flow-bin": "^0.22.0",
    "npm-run-all": "^1.5.1",
    "postcss-cssnext": "^2.4.0",
    "postcss-loader": "^0.8.1",
    "style-loader": "^0.13.0",
    "webpack": "^2.1.0-beta.4",
    "webpack-validator": "^1.0.0-beta.4"
  }
}
